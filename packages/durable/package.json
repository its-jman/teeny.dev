{
	"name": "@teeny.dev/durable",
	"version": "0.0.19",
	"type": "module",
	"sideEffects": false,
	"scripts": {
		"gen:wrangler": "wrangler types -c test-worker/wrangler.json",
		"build": "tsup",
		"test:types": "tsc",
		"test:vitest": "vitest",
		"test": "npm run test:types && npm run test:vitest -- --run",
		"release": "npm run build && npm run test && release-it"
	},
	"exports": {
		".": "./dist/index.js"
	},
	"files": [
		"dist",
		"src",
		"README.md",
		"package.json"
	],
	"prettier": "@jman.dev/prettier-config",
	"release-it": {
		"git": {
			"commitMessage": "teeny-durable@${version}",
			"tagName": "teeny-durable@${version}"
		},
		"github": {
			"releaseName": "teeny-durable@${version}",
			"release": true
		}
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20241230.0",
		"@jman.dev/prettier-config": "^0.1.8",
		"@trpc/client": "^10.45.2",
		"@trpc/server": "^10.45.2",
		"dayjs": "^1.11.13",
		"ky": "^1.7.4",
		"patch-package": "^8.0.0",
		"superjson": "^2.2.2",
		"tsup": "^8.3.5",
		"typescript": "^5.7.3",
		"vitest": "^2.1.8",
		"wrangler": "^3.100.0",
		"zod": "^3.24.1"
	},
	"peerDependencies": {
		"@cloudflare/workers-types": "^4.20230717.0",
		"@trpc/client": "^10.35.0",
		"@trpc/server": "^10.35.0"
	},
	"dependencies": {
		"ulid-workers": "^2.1.0"
	}
}
